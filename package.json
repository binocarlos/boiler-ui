{
  "name": "boiler-ui",
  "version": "1.0.5",
  "description": "common ui components for frontend apps",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run babel && npm run sass",
    "babel": "babel ./src --out-dir ./lib",
    "clean": "rimraf ./lib/*",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "sass": "cpx \"./src/**/*.scss\" ./lib"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bows": "^1.6.0",
    "deep-check-error": "^1.0.1",
    "immutability-helper": "^2.0.0",
    "object-path-immutable": "^0.5.1",
    "pluralize": "^3.1.0",
    "react-lite": "^0.15.29",
    "react-redux": "^5.0.1",
    "react-toolbox": "^1.3.3",
    "redux": "^3.6.0",
    "redux-little-router": "^12.1.1",
    "redux-logger": "^2.7.4",
    "redux-saga": "^0.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "ecstatic": "^2.1.0",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.2.0",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^0.14 || ~15.4.0",
    "react-addons-css-transition-group": "^0.14.0 || ~15.4.0",
    "react-dom": "^0.14.0 || ~15.4.0"
  },
  "license": "MIT"
}
